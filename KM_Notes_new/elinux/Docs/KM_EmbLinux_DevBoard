P8_28		LCD_PCLK		GPIO2_24[]	LCD_E
P8_29		LCD_HSYNC		GPIO2_23[]	LCD_RW
P8_30		LCD_DE			GPIO2_25[]	BUZZER
P8_31		LCD_DATA14		GPIO0_10 	USER LED					
P8_32		LCD_DATA15		GPIO0_11	USER SWITCH
P8_33		LCD_DATA13
P8_34		LCD_DATA11
P8_35		LCD_DATA12
P8_36		LCD_DATA10
